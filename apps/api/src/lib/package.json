{
  "name": "@api/lib",
  "private": true,
  "type": "module",
  "exports": {
    "./db": "./db/index.ts",
    "./db/factory": "./db/factory.ts",
    "./domain": "./domain/index.ts",
    "./error": "./error/index.ts",
    "./http": "./http/index.ts",
    "./logger": "./logger/index.ts",
    "./s3": "./s3/index.ts",
    "./s3/presign": "./s3/presign.ts",
    "./time": "./time/index.ts",
    "./types/result/db": "./types/result/db.ts"
  },
  "dependencies": {
    "@prisma/client": "^7.2.0",
    "hono": "^4.11.4",
    "neverthrow": "^8.2.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "prisma": "^7.2.0"
  },
  "scripts": {
    "check:type": "tsgo --noEmit",
    "db:generate": "cd db && bunx prisma generate",
    "db:migrate:dev": "cd db && bunx prisma migrate dev",
    "db:migrate:deploy": "cd db && bunx prisma migrate deploy",
    "db:migrate:reset": "cd db && bunx prisma migrate reset",
    "db:studio": "cd db && bunx prisma studio",
    "db:seed": "bun run db/seed.ts"
  }
